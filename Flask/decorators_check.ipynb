   "outputs": [],
   "source": [
    "def decorator(func):\n",
    "    def wrapper():\n",
    "        print(\"Hello!!\")\n",
    "        func()\n",
    "        print(\"Bye!!\")\n",
    "    return wrapper"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "39f2f501",
   "metadata": {},
   "outputs": [],
   "source": [
    "@decorator\n",
    "def my_func():\n",
    "    print(\"Decorator function\")"
   ]
  },
